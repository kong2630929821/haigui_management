.page{
    overflow-x: hidden;
    overflow-y: auto;
}
.empty{
    margin-top: 130px;
    justify-content: center;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.emptyImg{
    width: 190px;
    height: 190px;
}
.emptyText{
    height:40px;
    font-size:28px;
    color:rgba(136,136,136,1);
    line-height:40px;
    margin: 30px;
}
.shopping{
    width:300px;
    height:94px;
    border-radius:10px;
    border:2px solid #8D3FED;
    text-align: center;
    line-height: 94px;
    color: #8D3FED;
    font-size: 32px;
}
.edit{
    height:88px;
    font-size:24px;
    font-weight:500;
    color:rgba(83,169,248,1);
    line-height:88px;
    position: absolute;
    right: 10px;
    border: 20px solid transparent;
}
.goods{
    display: flex;
    margin: 30px 30px 30px 0;
    align-items: center;
}
.column{
    overflow: hidden;
    font-size: 24px;
    color:#888;
}
.goodsTitle{
    color:#222;
    word-break: break-all;
    margin-bottom: 10px;
}
.goodsFg{
    width: 160px;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 10px;
}
.row{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.goodsImg{
    width: 140px;
    height: 140px;
    margin-right: 20px;
    flex-shrink: 0;
}
.btn{
    font-size: 40px;
    color: #CCCCCC;
    width: 40px;
    text-align: center;
}
.total{
    font-size: 32px;
    margin: 0 30px;
}
.totalMoney{
    font-size:20px;
    color:#353535;
    flex: 1 0 0;
    text-align: right;
}
.pay{
    width:240px;
    height:98px;
    background:rgba(136,136,136,1);
    font-size:32px;
    font-weight:500;
    color:rgba(255,255,255,1);
    line-height:98px;
    margin-left: 30px;
    text-align: center;
}
.selectBox{
    width: 46px;
    height: 46px;
    border: 20px solid transparent;
    margin-left: 10px;
    flex-shrink: 0;
}
.bottom{
    position: fixed;
    bottom: 110px;
    width: 100%;
}
.active{
    background: #8A4AF3;
}