.imgBox{
    width: 100%;
    height: 100%;
    position: relative;
}
.imgItem{
    width: 100%;
    height: 100%;
}
.import-input{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
}
.addBox{
    font-size: 32px;
}
.addImg{
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
    display: flex;
    flex-direction: column;
}