.datePicker{
    width: 100%;
    max-width: 400px;
    position: relative;
}
.active{
    background-color: #fff;
    border:1px solid rgba(0,0,0,0.1);
    border-radius:4px;
}
.dateBox{
    position: absolute;
    top: 47px;
    left: 0;
    display: flex;
    z-index: 1;
}
.dateShow{
    display: flex;
    align-items: center;
    height: 37px;
    padding-left: 10px;
}
.inputBox{
    width: 325px;
}