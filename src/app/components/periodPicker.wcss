.datePicker{
    width: 225px;
    max-width: 360px;
    position: relative;
}
.active{
    background-color: #fff;
    border:1px solid rgba(0,0,0,0.1);
    border-radius:4px;
}
.dateBox{
    position: absolute;
    top: 58px;
    left: 0;
    display: flex;
    z-index: 1;
}
.dateShow{
    display: flex;
    align-items: center;
    height: 48px;
    padding: 0 10px;
}
.inputBox{
    width: 225px;
}